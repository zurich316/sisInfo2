<p id="notice"><%= notice %></p>

<h1>Almacen</h1>

<table>
  <thead>
    <tr>
      <th>Cod</th>
      <th>Nom</th>
      <th>Costo</th>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @almacens.each do |almacen| %>
      <tr>
        <td><%= almacen.cod %></td>
        <td><%= almacen.nom %></td>
        <td><%= almacen.costo %></td>
        <td><%= almacen.cantidad %></td>      
        <td><%= link_to 'Editar', edit_almacen_path(almacen) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo producto', new_almacen_path %>
<br>
<%= link_to 'Nueva venta por menor', new_ventum_path %>
<br>
<%= link_to 'Clientes', clients_path %>
