<p id="notice"><%= notice %></p>

<h1>Registro por venta</h1>

<table>
  <thead>
    <tr>
      <th>Producto</th>
      <th>Costo</th>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.producto %></td>
        <td><%= list.costo %></td>
        <td><%= list.cantidad %></td>
        <td><%= link_to 'Show', list %></td>
        <td><%= link_to 'Edit', edit_list_path(list) %></td>
        <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List', new_list_path %>
<br>
<%= link_to 'Ventas por menor', venta_path %>